@charset "utf-8";
/* CSS Document */
.viewedit .puce{ 
	cursor:pointer;
	float:left
}
.box-check {
	border: 1px solid silver; background-color:: #fff;
	width:16px;height:16px;float:left;
	margin-right:2px;
	-moz-border-radius: 6px; 
	-webkit-border-radius: 6px;
}
.li-text {
	margin-left:1px;
}
.box-disabled {
	opacity: .25; filter:Alpha(Opacity=25);
	background-color:#CCCCCC;
}
.viewedit li {
  	color: gray;
	clear:both;
 	list-style-type: none;
}
.viewedit ul { 
	margin:0px; 
}
.viewedit ul ul{ 
	margin-left:18px !important; 
}
.viewedit {
	padding: 5px 10px 0px;
	float:left;
	width:240px;
	height: 170px; 
	overflow: auto; 
	border: 1px solid #9E9E9E; 
	background: #fff; 
	color: #000;
	padding-top: 5px;
	margin-top:5px;
	margin-bottom:5px;
	-moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;

}
.selections {
	text-align: center;
	font-weight: bold;
	color: gray;
	background-color: #EEE;
	border: 1px solid #685840;
	opacity: .7;
}

#question-service_input .resumeview {
	width:208px;
}
.resumeview {
	float:left;
	width:150px;
	height: 170px; 
	overflow:auto; 
	border: 1px solid #9E9E9E; background: #fff; color: #000; margin-left:7px; 
	padding:5px 10px 0px;
	margin-top:5px;
	margin-bottom:5px;
	-moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;
}
.oneview {
	/*float:left;width:280px;*/
	background-image:url('images/cross-circle.png');
	background-repeat:no-repeat;
	background-position:right center;
	cursor: pointer;
	padding: 5px;
}
.oneview:hover {
	background-image:url('images/cross-circle.png');
	background-repeat:no-repeat;
	background-position:right center;
}
#loading {
	background-image:url('images/loading.gif');
	background-repeat:no-repeat;
	background-position:center center;
}
