.label-rotated
{
	float: left;
	text-align: center;
	margin: 5px 0px 0 -7px;
	padding: 2px;
	
	transform:rotate(-55deg);
	-ms-transform:rotate(-55deg); /* Internet Explorer */
	-moz-transform:rotate(-55deg); /* Firefox */
	-webkit-transform:rotate(-55deg); /* Safari and Chrome */
	-o-transform:rotate(-55deg); /* Opera */
}

.block-rotated
{
	
	transform:rotate(-15deg);
	-ms-transform:rotate(-15deg); /* Internet Explorer */
	-moz-transform:rotate(-15deg); /* Firefox */
	-webkit-transform:rotate(-15deg); /* Safari and Chrome */
	-o-transform:rotate(-15deg); /* Opera */
}

.block-more-rotated
{
	
	transform:rotate(-5deg);
	-ms-transform:rotate(-5deg); /* Internet Explorer */
	-moz-transform:rotate(-5deg); /* Firefox */
	-webkit-transform:rotate(-5deg); /* Safari and Chrome */
	-o-transform:rotate(-5deg); /* Opera */
}

.no-rotated
{
	
	transform:rotate(0deg);
	-ms-transform:rotate(0deg); /* Internet Explorer */
	-moz-transform:rotate(0deg); /* Firefox */
	-webkit-transform:rotate(0deg); /* Safari and Chrome */
	-o-transform:rotate(0deg); /* Opera */
}

.block-inv-rotated
{
	
	transform:rotate(20deg);
	-ms-transform:rotate(20deg); /* Internet Explorer */
	-moz-transform:rotate(20deg); /* Firefox */
	-webkit-transform:rotate(20deg); /* Safari and Chrome */
	-o-transform:rotate(20deg); /* Opera */
}

.block-inv-more-rotated
{
	
	transform:rotate(5deg);
	-ms-transform:rotate(5deg); /* Internet Explorer */
	-moz-transform:rotate(5deg); /* Firefox */
	-webkit-transform:rotate(5deg); /* Safari and Chrome */
	-o-transform:rotate(5deg); /* Opera */
}

