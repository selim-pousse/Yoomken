.widescreen.show-picture {
	position: fixed;
	left: 0;
	width: 100%;
	background: #3A362F; /*#3A3937;*/
}

.watch {
	overflow: hidden;
}

.watch .watch-inner {
	line-height: 0;
	position: relative;
	margin: 0 auto;
}

.widescreen .ls-nav-prev, .widescreen .ls-nav-next {
	width: 40px;
	height: 40px;
	z-index: 10000;
	top: 50%;
	margin-top: -20px;
	position: absolute;
}

.widescreen .close {
	background: url(images/modal-close-white.png) center center !important;
	background-repeat: no-repeat !important;
	width: 16px;
	height: 16px;
	z-index: 10000;
	top:10px;
	right: 10px;
	position: absolute;
}
.widescreen .ls-nav-next {
	right: 10px;
}

.widescreen .ls-nav-prev {
	left: 10px;
}

.ls-nav-next {
	background: #292827 url(images/slider_right.png) center center !important;
	background-repeat: no-repeat !important;
	height: 65px !important;
	width: 65px !important;
	opacity: 0.7;
	margin-right: 30px;
	border-radius: 5px;
}

.ls-nav-prev {
	background: #292827 url(images/slider_left.png) center center !important;
	background-repeat: no-repeat !important;
	height: 65px !important;
	width: 65px !important;
	opacity: 0.7;
	margin-left: 30px;
	border-radius: 5px;
}


