#contactable #contactable_inner 
{
	background-image:url(images/test_feedback.png);
	color:#FFFFFF;
	background-color:#333333;
	cursor:pointer;
	height:102px;
	margin-left:-10px; 
	overflow:hidden;
	position:relative;
	*position:absolute;
	text-indent:-100000px;
	top:205px;
	*margin-top:10px;
	width:44px;
	z-index:100000;
	float:left;
}

#contactable p
{
	color:white;
}
#contactable
{
	z-index : 12;
	left:0;
	top:70px;
	position:fixed;
	margin-left:-400px;
}

#contactable #contactForm {
	background-color: #685840;
	border:1px solid #EBEBEB;
	color:#FFFFFF;
	left:0;
	/**margin-left:-434px;
	margin-top:-160px;*/
	overflow:hidden;
	padding:0px;
	position:relative;
	z-index:99;
	float:left;
}

#contactable .border-white
{
	border: 2px solid white;
	/*height: 450px;*/
	width: 360px;
	padding: 0 20px;
	min-height: 335px;
}

#contactable form#contactForm input[type="text"],#contactable form#contactForm textarea {
	background:#F1EFEB none repeat scroll 0 0;
	outline-style:none;
	outline-width:medium;
	width:345px;
	padding:5px;
	border:1px solid #dfdfdf;
	font-size:1em;
	margin-bottom:10px;
	-moz-border-radius: 4px/*{cornerRadius}*/; -webkit-border-radius: 4px/*{cornerRadius}*/; border-radius: 4px/*{cornerRadius}*/;
}
#contactable form#contactForm textarea
{
	height: 200px;
}

#contactable form#contactForm input:focus,#contactable form#contactForm textarea:focus
{
	background-color: white;	
}

#contactable form#contactForm .submit {
	width:360px;
	float:none;
	/*background: #F8D080 none repeat scroll 0px 0px;
	border: 3px solid #F8D088;
	outline-style:none;
	outline-width:medium;
	
	padding:5px;
	outline-color:-moz-use-text-color;
	font-size:1em;
	cursor:pointer;
	color:#FFF;
	text-transform:uppercase;
	font-weight:bolder;
	margin-top:10px;*/
}

#contactable form#contactForm p {
	width:325px;
	font-size:0.9em;
}

#contactable form#contactForm .disclaimer {
	*margin-left:20px;
	font-size: 12px;
	color: #E4DDD3;
}

#contactable #contactForm .red {
	color:	#F5410F;
}

#contactable #overlay {
	background-color:#666666;
	display:none;
	height:100%;
	left:0;
	margin:0;
	padding:0;
	position:absolute;
	top:0;
	width:100%;
	z-index:0;
} 
#contactable .error { background-color: #EDBE9C; }

#contactable #name.error { background-color: #EDBE9C; } 
#contactable #email.error { background-color: #EDBE9C; }
#contactable #message.error { background-color: #EDBE9C; }

#contactable form#contactForm label{
	*margin-left:20px;
}

#contactable form#contactForm #loading {
	background: url(images/ajax-loader.gif) no-repeat;
	width:55px;
	height:55px;
	margin: 100px auto;
	display:none;
}

#contactable #callback {
	font-size:1.1em;
	color: #FFF;
	width:325px;
	margin: 100px auto;	
	display:none;
}


#contactable .holder {
	margin:0 auto;
	*margin-left:20px;
	padding: 20px 0px;	
}	
